package entities;

import items.ItemPlasmaBolt;
import net.minecraft.client.renderer.entity.RenderSnowball;
import net.minecraft.item.Item;
import cpw.mods.fml.relauncher.Side;
import cpw.mods.fml.relauncher.SideOnly;

@SideOnly(Side.CLIENT)
public class renderplasmabolt extends RenderSnowball
{
public renderplasmabolt(ItemPlasmaBolt Plasma_Bolt) {
this(Plasma_Bolt, 0);
}

public renderplasmabolt(ItemPlasmaBolt Plasma_Bolt, int par2) {
super(Plasma_Bolt, par2);
}

// now you can override the render methods if you want
// call super to get the original functionality, and/or add some stuff of
//your own
// I'll leave that up to you to experiment with
}
